{
  "name": "gen-workout-backend",
  "version": "1.0.0",
  "description": "Backend for AI-integrated workout tracking app",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "type-check": "tsc --noEmit",
